#include<iostream>
using namespace std;

int main(){
    int A,B;
    cout<<"enter A :";cin>>A;
    cout<<"enter B :";cin>>B;
    
    // Exculsive OR
    cout << "A ^ B = " << (A ^ B) << endl;
  
    // OR
    cout << "A | B = " << (A | B) << endl;
  
    // AND
    cout << "A & B = " << (A & B) << endl;

    return 0;
}
